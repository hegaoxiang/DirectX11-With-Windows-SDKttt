<Window x:Class="GEditor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GEditor"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Background="{DynamicResource {x:Static SystemColors.HotTrackBrushKey}}">
        <WindowsFormsHost x:Name="wfHost" HorizontalAlignment="Left" Height="215" Margin="190,115,0,0" VerticalAlignment="Top" Width="405" ChildChanged="wfHost_ChildChanged" Background="{DynamicResource {x:Static SystemColors.MenuTextBrushKey}}"/>

    </Grid>
</Window>
